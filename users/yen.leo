    while (y &lt; 6):
        print ("y = " + str(y),) # the , forces printing on the same line
        y = y + 1
        z = 1
        while (z &lt; 6):
            print ("z = " + str(z),) # the , forces printing on the same line
            z = z + 1
        print() # prints a new line
