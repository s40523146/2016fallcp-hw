def main(argv = None):
    if argv is None:
        argv = sys.argv

    xml = False
    sql = False
    web = False
    newTuning = False
    conn = None
    handle = None
    webpage = None
    webmulti = False
    wantnotelist = False
    scaleopt = 100
    dr = "" # directory in which each chord 'library' will reside
    tuningopt = ""
    tname = ""
    ttuning = ""
    webopt =""

    # parse command line options
    try:
        opts, args = getopt.getopt(sys.argv[1:], "hxdw:t:ns:", ["help","xml","database","web=","tuning=","notelist","scale="])
    except getopt.error, msg:
        print(msg)
        print("for help use --help")
        sys.exit(2)
    # process options
    for o, a in opts:
        if o in ("-h", "--help"):
            print __doc__
            sys.exit(0)
        if o in ("-x", "--xml"):
            xml = True
        if o in ("-d", "--database"):
            sql = True
        if o in ("-w", "--web"):
            web = True
            webopt = a
        if o in ("-t", "--tuning"):
            tuningopt = a
        if o in ("-n", "--notelist"):
            wantnotelist = True
        if o in ("-s", "--scale"):
            scaleopt = a

    if not web:
        print("No web page will be produced")
    if not xml:
        print("No XML metafile will be produced")
    if not sql:
        print("No chords will be inserted in a database")

    if tuningopt != "": # a tuning name and tuning should be in argument given
        #print("New Tuning: " + tuningopt)
        # split the given option string into name and tuning
        if tuningopt.find(' ') == -1: # must be two strings
            print("Tuning option must include a useful name, a space, followed by a tuning string of at least six characters")
            exit()            
        tname, ttuning = tuningopt.split()
