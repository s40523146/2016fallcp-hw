# 'normal' guitar tuning
defaultTuning = ["E","A","D","G","B","E"]

# 'current' tuning, unless this is changed with call to adjustTuning
currentTuning = ["E","A","D","G","B","E"]

handed = "R" # player's handedness - default to right-handed

